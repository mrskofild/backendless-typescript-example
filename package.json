{
  "name": "backendless-typescript-example",
  "version": "1.0.0",
  "scripts": {
    "debug": "coderunner debug",
    "deploy": "cross-env NODE_ENV=develop coderunner deploy -- -q",
    "set-configs": "cross-env NODE_ENV=develop ts-node --require tsconfig-paths/register bin/set-config",
    "lint": "eslint app bin scripts test --fix",
    "test:unit": "NODE_ENV=test ts-mocha --paths -p ./tsconfig.json --parallel --jobs 10 --full-trace \"test/unit/**/*.test.ts\"",
    "test:integration": "NODE_ENV=test ts-mocha --paths -p ./tsconfig.json --parallel --jobs 10 --full-trace \"test/integration/**/*.test.ts\" --timeout 999999",
    "check:circular": "madge --circular --extensions ts --ts-config ./tsconfig.json ./app",
    "build": "tsc --build ./tsconfig.json && tsc-alias"
  },
  "devDependencies": {
    "@types/chai-arrays": "^2.0.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-http": "^4.2.0",
    "@types/dirty-chai": "^2.0.2",
    "@types/expect": "^24.3.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/moment": "^2.13.0",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-eslint": "^10.1.0",
    "backendless-coderunner": "^6.1.6",
    "chai": "^4.3.3",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-match-pattern": "^1.3.0",
    "cross-env": "^5.2.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "faker": "^5.4.0",
    "husky": "^4.3.8",
    "jest": "^28.0.3",
    "lint-staged": "^10.5.3",
    "madge": "^4.0.2",
    "mocha": "^10.0.0",
    "mock-require": "^3.0.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "string-template": "^1.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "license": "MIT",
  "dependencies": {
    "backendless": "^6.2.0",
    "backendless-console-sdk": "^2.11.0",
    "dns-cache": "^2.0.0",
    "encoding": "^0.1.13",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "query-string": "^6.13.8"
  },
  "main": "bin/cli/index.ts",
  "types": "app/types/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run check:circular && npm run lint &&  npm run test:unit"
    }
  }
}